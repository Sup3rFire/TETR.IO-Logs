{
  "manifest_version": 2,
  "name": "TETR.IO Logs",
  "version": "0.18.6",
  "description": "Logging for TETR.IO",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "storage",
    "*://*.tetr.io/*"
  ],

  "page_action": {},

  "background": {
    "scripts": ["index.js"]
  }
}
